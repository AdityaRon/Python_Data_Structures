    "def rev(x):\n",
    "    if len(x) <= 1:\n",
    "        return x\n",
    "    length = len(x)-1\n",
    "#     print(length)\n",
    "#     print(x[length])\n",
    "#     print(x[:length])\n",
    "    k = x[length]\n",
    "    l = x[:length]\n",
    "#     print(k,l)\n",
    "    return (k + rev(l))\n",
    "    "
   ]
